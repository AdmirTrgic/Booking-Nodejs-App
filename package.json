{
  "name": "natours",
  "version": "1.0.0",
  "description": "node, express, mongoDB",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "SET NODE_ENV=production && nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel serve --no-hmr ./public/js/index.js --dist-dir ./public/js/bundled",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled"
  },
  "author": "Admir Trgic",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.19.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-csp": "^0.1.3",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^5.5.0",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "html-to-text": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.12",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nodemailer": "^6.7.0",
    "nodemailer-sendgrid": "^1.0.3",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.9",
    "sharp": "^0.29.3",
